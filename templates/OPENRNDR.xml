  <template name="v2" value="Vector2($X$, $Y$)" description="Vector2" toReformat="false" toShortenFQNames="true">
    <variable name="X" expression="" defaultValue="0.0" alwaysStopAt="true" />
    <variable name="Y" expression="" defaultValue="0.0" alwaysStopAt="true" />
