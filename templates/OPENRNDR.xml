<templateSet group="OPENRNDR">
  <template name="ora" value="fun main() = application { &#10;    program {&#10;        extend {&#10;        }&#10;    }&#10;}" description="Create OPENRNDR application" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="KOTLIN_TOPLEVEL" value="true" />
    </context>
  </template>
  <template name="diwt" value="drawer.isolatedWithTarget($RT$) {&#10;$END$&#10;}" description="Drawer isolated with target" toReformat="false" toShortenFQNames="true">
    <variable name="RT" expression="variableOfType(&quot;org.openrndr.draw.RenderTarget&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
  <template name="rtcb" value="renderTarget($WIDTH$, $HEIGHT$) {&#10;    colorBuffer()&#10;}" description="RenderTarget with ColorBuffer" toReformat="false" toShortenFQNames="true">
    <variable name="WIDTH" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="HEIGHT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
  <template name="cb" value="colorBuffer($WIDTH$, $HEIGHT$)" description="ColorBuffer" toReformat="false" toShortenFQNames="true">
    <variable name="WIDTH" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="HEIGHT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
  <template name="bt" value="buildTransform {&#10;    $END$&#10;}" description="Build transform" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
  <template name="bc" value="contour {&#10;    $END$&#10;}" description="Build contour" toReformat="false" toShortenFQNames="true">
    <context>
